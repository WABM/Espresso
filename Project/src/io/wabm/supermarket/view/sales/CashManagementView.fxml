<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="578.0" prefWidth="669.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="io.wabm.supermarket.controller.sales.CashManagementController">
   <center>
      <TableView fx:id="tableView" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
         <columns>
            <TableColumn fx:id="CashRegisterID" prefWidth="75.0" text="机号" />
            <TableColumn fx:id="EmployeeID" prefWidth="75.0" text="收银员" />
            <TableColumn fx:id="MoneyIN" prefWidth="75.0" text="初始金额" />
            <TableColumn fx:id="MoneyOUT" prefWidth="75.0" text="实收金额" />
            <TableColumn fx:id="MoneyShould" prefWidth="75.0" text="营后金额" />
         </columns>
      </TableView>
   </center>
   <top>
      <ToolBar nodeOrientation="LEFT_TO_RIGHT" prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <items>
            <Button fx:id="accountingButton" mnemonicParsing="false" onAction="#accountingPressed" text="核算" />
         </items></ToolBar>
   </top>
</BorderPane>
