<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="578.0" prefWidth="669.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="io.wabm.supermarket.controller.warehouse.CommodityStockManagementController">
   <center>
      <TableView prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
         <columns>
            <TableColumn prefWidth="75.0" text="商品编号" />
            <TableColumn prefWidth="75.0" text="条形码" />
            <TableColumn prefWidth="75.0" text="名称" />
            <TableColumn prefWidth="75.0" text="分类" />
            <TableColumn prefWidth="75.0" text="规格" />
            <TableColumn prefWidth="75.0" text="单位" />
            <TableColumn prefWidth="75.0" text="库存" />
            <TableColumn prefWidth="75.0" text="日均销量" />
            <TableColumn prefWidth="75.0" text="操作" />
         </columns>
      </TableView>
   </center>
   <top>
      <ToolBar nodeOrientation="RIGHT_TO_LEFT" prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <items>
            <Button layoutX="10.0" layoutY="11.0" mnemonicParsing="false" text="查找" />
            <Pane HBox.hgrow="ALWAYS" />
            <Button fx:id="orderReceiveButton" layoutX="10.0" layoutY="11.0" mnemonicParsing="false" onAction="#orderReceiveButtonPressed" text="待收货订单 (9)" />
            <Button fx:id="purchaseFormButton" mnemonicParsing="false" onAction="#purchaseFormButtonPressed" text="需补货商品 (42)" />
         </items>
      </ToolBar>
   </top>
</BorderPane>
