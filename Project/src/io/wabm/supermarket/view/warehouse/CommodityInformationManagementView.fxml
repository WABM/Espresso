<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<BorderPane prefHeight="578.0" prefWidth="669.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="io.wabm.supermarket.controller.warehouse.CommodityInformationController">
   <center>
      <TableView fx:id="tableView" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
         <columns>
            <TableColumn fx:id="idColumn" prefWidth="75.0" text="商品编号" />
            <TableColumn fx:id="barCodeColumn" prefWidth="75.0" text="条形码" />
            <TableColumn fx:id="nameColumn" prefWidth="75.0" text="名称" />
            <TableColumn fx:id="classificationColumn" prefWidth="75.0" text="分类" />
            <TableColumn fx:id="specificationColumn" prefWidth="75.0" text="规格" />
            <TableColumn fx:id="unitColumn" prefWidth="75.0" text="单位" />
            <TableColumn fx:id="priceColumn" prefWidth="75.0" text="单价" />
            <TableColumn fx:id="deliverySpecificationColumn" prefWidth="75.0" text="配送规格" />
            <TableColumn fx:id="createTimestampColumn" prefWidth="75.0" text="建档时间" />
         </columns>
      </TableView>
   </center>
   <top>
      <ToolBar nodeOrientation="RIGHT_TO_LEFT" prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <items>
            <Button fx:id="searchButton" layoutX="10.0" layoutY="11.0" mnemonicParsing="false" text="查询" />
            <Button fx:id="modifyButton" layoutX="10.0" layoutY="11.0" mnemonicParsing="false" text="修改" />
            <Button fx:id="deleteButton" layoutX="58.0" layoutY="11.0" mnemonicParsing="false" text="删除" />
            <Button fx:id="addButton" layoutX="106.0" layoutY="11.0" mnemonicParsing="false" text="添加" />
            <Pane HBox.hgrow="ALWAYS" />
            <Button fx:id="backButton" mnemonicParsing="false" onAction="#backButtonPressed" text="返回" />
         </items>
      </ToolBar>
   </top>
</BorderPane>
