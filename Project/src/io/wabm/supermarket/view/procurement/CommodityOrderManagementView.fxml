<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="578.0" prefWidth="669.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="io.wabm.supermarket.controller.procurement.CommodityOrderManagementController">
   <center>
      <TableView prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
         <columns>
            <TableColumn prefWidth="75.0" text="订单编号" />
            <TableColumn prefWidth="75.0" text="供应商" />
            <TableColumn prefWidth="75.0" text="订单时间" />
            <TableColumn prefWidth="75.0" text="订单状态" />
            <TableColumn prefWidth="75.0" text="操作" />
         </columns>
      </TableView>
   </center>
   <top>
      <ToolBar nodeOrientation="RIGHT_TO_LEFT" prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <items>
            <Button fx:id="newButton" layoutX="10.0" layoutY="11.0" mnemonicParsing="false" onAction="#newButtonPressed" text="新建订单" />
            <Pane HBox.hgrow="ALWAYS" />
            <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" text="已完成订单" />
            <Button layoutX="543.0" layoutY="10.0" mnemonicParsing="false" text="待收货订单" />
            <Button layoutX="543.0" layoutY="10.0" mnemonicParsing="false" text="未付款订单" />
         </items>
      </ToolBar>
   </top>
</BorderPane>
