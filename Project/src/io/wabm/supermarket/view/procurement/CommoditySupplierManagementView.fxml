<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>




<BorderPane prefHeight="578.0" prefWidth="669.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="io.wabm.supermarket.controller.procurement.CommoditySupplierManagementViewController">

   <center>
      <TableView fx:id="tableView" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
         <columns>
            <TableColumn fx:id="idColumn" prefWidth="88.0" text="编号" />
            <TableColumn fx:id="nameColumn" prefWidth="179.0" text="名称" />
            <TableColumn fx:id="representativeColumn" prefWidth="89.0" text="联系人" />
            <TableColumn fx:id="phoneColumn" prefWidth="178.0" text="联系电话" />
            <TableColumn fx:id="addressColumn" prefWidth="176.0" text="货源地" />
         </columns>
      </TableView>
   </center>
   <top>
      <ToolBar nodeOrientation="RIGHT_TO_LEFT" prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <items>
            <Button fx:id="modifyButton" mnemonicParsing="false" onAction="#modifyButtonPressed" text="修改" />
            <Button fx:id="deleteButton" mnemonicParsing="false" onAction="#deleteButtonPressed" text="删除" />
            <Button fx:id="newButton" mnemonicParsing="false" onAction="#newButtonPressed" text="添加" />
            <Pane HBox.hgrow="ALWAYS" />
            <Button layoutX="543.0" layoutY="10.0" mnemonicParsing="false" text="查找" />
            <TextField />
         </items>
      </ToolBar>
   </top>
</BorderPane>
